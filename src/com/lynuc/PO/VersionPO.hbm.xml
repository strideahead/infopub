<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="true">
  <class  name="com.lynuc.PO.VersionPO" table="version">
    <id name="gguid" type="java.lang.String" length="50">
      <column name="gguid" not-null="true"/>
      <generator class="assigned"/>
    </id>
    <property name="version_name" type="java.lang.String">
      <column name="version_name" length="50" not-null="false"/>
    </property>
    <property name="is_soft" type="java.lang.String">
      <column  name="is_soft" length="50" not-null="false"/>
    </property>
    <property name="is_publish" type="java.lang.String">
      <column  name="is_publish" length="50" not-null="false"/>
    </property>
 </class>
</hibernate-mapping>
